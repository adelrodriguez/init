{
  "name": "@this/auth",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./password": "./src/password.ts",
    "./nextjs": "./src/nextjs.ts"
  },
  "license": "MIT",
  "scripts": {
    "check": "biome check --write src",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "generate": "pnpx @better-auth/cli generate --config src/index.ts --output ../db/src/schema/auth2.ts",
    "format": "biome format --write src",
    "lint": "biome lint --fix --unsafe src",
    "secret": "pnpx @better-auth/cli@latest secret",
    "test": "vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@tooling/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@noble/hashes": "^1.6.1",
    "@oslojs/encoding": "^1.1.0",
    "@this/common": "workspace:^",
    "@this/db": "workspace:^",
    "@this/env": "workspace:^",
    "better-auth": "^1.0.5",
    "server-only": "catalog:"
  }
}
