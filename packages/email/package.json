{
  "name": "@this/email",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/templates/*.tsx"
  },
  "scripts": {
    "build": "email build --dir src/templates",
    "check": "biome check --write src",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "email dev --dir src/templates --port 8081",
    "export": "email export --dir src/templates",
    "format": "biome format --write src",
    "lint": "biome lint --fix --unsafe src",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@react-email/components": "0.0.32",
    "@react-email/render": "^1.0.4",
    "@this/common": "workspace:*",
    "@this/env": "workspace:^",
    "@this/observability": "workspace:*",
    "@this/queue": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:",
    "resend": "4.1.1"
  },
  "devDependencies": {
    "@tooling/tsconfig": "workspace:*",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react-email": "3.0.6",
    "typescript": "catalog:"
  }
}
